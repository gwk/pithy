{
  "search.exclude": {
    ".git": true,
    "_build": true,
    "_misc": true,
    "pkg/crafts/**": true,
    "pkg/iotest/**": true,
    "pkg/legs/**": true,
    "pkg/pithy/**": true,
    "pkg/pithytools/**": true,
    "pkg/tolkien/**": true,
    "pkg/utest/**": true,
    "pkg/wu/**": true
  },
  "mypy.targets": [
    "crafts",
    "iotest",
    "legs",
    "pithy",
    "pithytools",
    "test",
    "tolkien",
    "tools",
    "utest",
    "wu"
  ],
  "python.analysis.importFormat": "relative",
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
  },
  "files.exclude": {
    "dist/**": true,
    "pkg/crafts/**": true,
    "pkg/iotest/**": true,
    "pkg/legs/**": true,
    "pkg/pithy/**": true,
    "pkg/pithytools/**": true,
    "pkg/tolkien/**": true,
    "pkg/utest/**": true,
    "pkg/wu/**": true
  },
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.exclude": [
    "_build/**",
    "_misc/**"
  ],
  "python.analysis.include": [
    "crafts",
    "iotest",
    "pithy",
    "test-meta",
    "test",
    "wu"
  ],
  "python.analysis.typeCheckingMode": "off",
  "[json]": {
    "editor.formatOnSave": false
  },
  "[jsonc]": {
    "editor.formatOnSave": false
  },
  "json.format.enable": false,
}
